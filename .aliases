# Custom Username 
# PS1='[\u@fedora \W]$ '

# Setting neovim as default editor
export EDITOR=nvim

# Aliases
alias c="clear"
alias nv="nvim"
alias ls="ls --color=auto"
alias ll="ls -lav --ignore=.. --ignore=."
alias :q="exit"
alias wifi-password="nmcli device wifi show-password ifname wlp3s0"
alias system="fastfetch"
alias bat="acpi"
